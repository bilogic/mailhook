{
    "require": {
        "zbateson\/mail-mime-parser": "^2.4",
        "avexsoft\/core": "^10"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9",
        "avexsoft\/dev-tools": "^10",
        "avexsoft\/cheatsheet": "^10",
        "barryvdh\/laravel-debugbar": "*",
        "laravel\/telescope": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "..\/..\/repo-10\/avexcode.com\/*\/*"
        },
        {
            "type": "path",
            "url": "..\/..\/package-repo\/packages\/*\/*",
            "canonical": false
        },
        {
            "type": "path",
            "url": "..\/..\/package-repo\/packages-no-origin\/*\/*"
        },
        {
            "type": "path",
            "url": "..\/..\/package-repo\/packages-deprecated\/*\/*"
        },
        {
            "type": "composer",
            "url": "https:\/\/satis.avexcode.com"
        }
    ]
}